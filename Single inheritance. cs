using System;
using System.IO;
using System.Linq;
using System.Collections.Generic;

namespace CSharp_Shell
{
	class stud
	{
		public string name;

		public void display()
		{
			Console.WriteLine("I am an stud");
		}

	}
	class exam : stud
	{
		public void getName()
		{
			Console.WriteLine("My paper name is " + name);
		}
	}

	class Program
	{

		static void Main(string[] args)
		{
			exam s1 = new exam();
			s1.name = "C#.Net";
			s1.display();
			s1.getName();

			Console.ReadLine();
		}

	}
}

